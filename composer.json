{
  "name": "codehunger/laravel-activity-logger",
  "description": "Laravel Activity Logger Package",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Junaid Iqbal Marar",
      "email": "junaidiqbalmrar@gmail.com"
    }
  ],
  "require": {
    "php": ">=8.0"
  },
  "autoload": {
    "psr-4": {
      "CodeHunger\\LaravelActivityLogger\\": "src/"
    }
  },
  "extra": {
    "laravel": {
